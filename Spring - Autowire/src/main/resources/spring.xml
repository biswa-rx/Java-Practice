<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


<!--    by type-->
<!--    <bean id="alien1" class="org.example.Alien" autowire="byType">-->

<!--    by name-->
    <bean id="alien1" class="org.example.Alien" autowire="byType">

        <property name="age" value="21"/>

<!--        explicitly mention is the highest priority-->
<!--	    <property name="com" ref="com1"/>-->

    </bean>

    <bean id="com1" class="org.example.Desktop" primary="true">
    </bean>

    <bean id="com2" class="org.example.Laptop" lazy-init="true">
    </bean>





<!--    Inner bean-->
    <bean id="alien2" class="org.example.Alien" autowire="byType">

        <property name="age" value="21"/>

        <property name="com">
            <bean id = "com3" class = "org.example.Laptop" primary="true">
            </bean>
        </property>

    </bean>

</beans>